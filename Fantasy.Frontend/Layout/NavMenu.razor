<MudNavMenu>
    <MudStack AlignItems="AlignItems.Center">
        <MudImage Src="images/Logo.jpg" Width="200" Class="p-3" />
    </MudStack>    
    <MudDivider />
    <MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Rounded.Home">@L["IUHome"]</MudNavLink>
    <MudDivider />
     <AuthorizeView Roles="Admin">
        <Authorized>
            <!-- Admin Submenu  -->
            <MudNavGroup Title="@L["Admin Options"]" Icon="@Icons.Material.Filled.AdminPanelSettings">
                <MudNavLink Href="/currencies" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Public">@L["Currencies"]</MudNavLink>
                <MudNavLink Href="/countries" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Public">@L["Countries"]</MudNavLink>
                <MudNavLink Href="/tenants" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Groups">@L["Tenants"]</MudNavLink>
                <MudNavLink Href="/subscriptions" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Star">@L["Subscriptions"]</MudNavLink>
                <MudNavLink Href="/enterprices" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Star">@L["Enterprices"]</MudNavLink>
                <MudNavLink Href="/users" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.People">@L["Users"]</MudNavLink>
            </MudNavGroup>
            <MudDivider /> 
         </Authorized>
    </AuthorizeView>
    <AuthorizeView>
        <MudNavLink Href="/groups" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.SportsSoccer">@L["MyGroups"]</MudNavLink>
        <MudDivider />
    </AuthorizeView> 
    <MudNavLink Href="/about" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Info">@L["IUAbout"]</MudNavLink>
</MudNavMenu>
